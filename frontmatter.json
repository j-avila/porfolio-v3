{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321/",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/src/pages/posts"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "pubDate",
          "name": "pubDate",
          "type": "datetime"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "image",
          "name": "image",
          "type": "fields",
          "fields": [
            {
              "title": "url",
              "name": "url",
              "type": "string"
            },
            {
              "title": "alt",
              "name": "alt",
              "type": "string"
            }
          ]
        },
        {
          "title": "cover",
          "name": "cover",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "heroImage",
          "name": "heroImage",
          "type": "image"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.website.host": "http://joseavila.dev/"
}